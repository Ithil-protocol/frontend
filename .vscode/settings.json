    "next.config.js": false,
    "cspell.json": false,
    ".yarnrc.yml": false,
    ".prettierrc.json": false,
    ".prettierignore": false,
    ".lintstagedrc.js": false,
    ".eslintrc.json": false,
    ".gitignore": false,
    ".editorconfig": false,
    ".env.example": false,
    "node_modules": false,
