{
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.tabSize": 2,
  "explorerExclude.backup": {},
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "yarn.lock": true,
    "wagmi.config.ts": true,
    "tsconfig.json": true,
    "tailwind.config.js": true,
    "README.md": true,
    "postcss.config.js": true,
    "package.json": true,
    "next.config.js": true,
    "cspell.json": true,
    ".yarnrc.yml": true,
    ".prettierrc.json": true,
    ".prettierignore": true,
    ".lintstagedrc.js": true,
    ".eslintrc.json": true,
    ".gitignore": true,
    ".editorconfig": true,
    ".env.example": true,
    "node_modules": true,
    ".yarn": true,
    ".vscode": true,
    "public": true,
    ".husky": true,
    "tsconfig.tsbuildinfo": true,
    "next-env.d.ts": true,
    ".next": true,
    ".trashcan": true,
    "pnpm-lock.yaml": true,
    ".svgrrc.json": true,
    ".npmrc": true,
    ".env.local": true
  },
  "prettier.configPath": ".prettierrc.json",
  "prettier.endOfLine": "lf",
  "prettier.useTabs": false,
  "typescript.tsdk": "node_modules/typescript/lib"
}
